sensor buttonone: 9
sensor buttontwo: 10
actuator buzzer: 8

# Declaring states
stateone {
    buzzer <= HIGH
    buttonone is LOW or buttontwo is LOW => statetwo
}

-> statetwo {
    buzzer <= LOW
    buttonone is HIGH and buttontwo is HIGH => stateone
}


# variante
signal statetwo on led

application exo

